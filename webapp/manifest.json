{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.todo",
		"type": "application"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "sap.ui.demo.todo.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.todo.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"controlAggregation": "pages",
				"controlId": "navContainer",
				"viewType": "XML",
				"transition": "slide",
				"async": true
			},
			"routes": [{
					"name": "home",
					"pattern": "/home",
					"target": "home"
				}
			],
			"targets": {
				"home": {
					"viewName": "App",
					"viewPath": "sap.ui.demo.home"
				}
			}
		}
	}
}
